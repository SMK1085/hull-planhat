{
  "name": "hull-planhat",
  "version": "0.1.1",
  "description": "Hull connector for planhat.com",
  "main": "index.ts",
  "scripts": {
    "start": "npm run serve",
    "serve": "pm2-runtime start ecosystem.config.js --env production",
    "test": "jest --env=node --colors --coverage --no-cache",
    "build:js": "babel src --out-dir build --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "tslint src/**/*.{ts,tsx}",
    "serve-debug": "nodemon --inspect build/index.js",
    "ngrok": "ngrok http 8093 --subdomain=hull-planhat",
    "postinstall": "npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SMK1085/hull-planhat.git"
  },
  "keywords": [
    "Hull",
    "Planhat",
    "Integration"
  ],
  "author": {
    "email": "sven@hull.io",
    "name": "Sven Maschek",
    "url": "https://www.hull.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SMK1085/hull-planhat/issues"
  },
  "homepage": "https://github.com/SMK1085/hull-planhat#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "hull": "^0.13.20",
    "lodash": "^4.17.15",
    "newrelic": "^5.13.1",
    "pm2": "^3.5.1",
    "typescript": "^3.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-typescript": "7.3.3",
    "@babel/runtime": "^7.9.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.149",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.12.30",
    "@types/winston": "^2.4.4",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "nock": "10.0.6",
    "nodemon": "1.19.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1"
  }
}
